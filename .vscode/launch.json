{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "server",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Boligmappa.Server/bin/Debug/net6.0/Boligmappa.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Boligmappa.Server",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "console-app-1",
            "type": "coreclr",
            "request": "launch",
            "externalConsole": true,
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Boligmappa.ConsoleApp1/bin/Debug/net6.0/Boligmappa.ConsoleApp1.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Boligmappa.ConsoleApp1",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "console-app-2",
            "type": "coreclr",
            "request": "launch",
            "externalConsole": true,
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Boligmappa.ConsoleApp2/bin/Debug/net6.0/Boligmappa.ConsoleApp2.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Boligmappa.ConsoleApp2",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}